#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"
#include "pwmip.h"
#include "xparameters.h"

int main()
{
    init_platform();
    u32 period = 20000000;
    u32 duty = 10000000;
    print("Hello World\n\r");
    PWMIP_mWriteReg(XPAR_PWMIP_0_S_AXI_BASEADDR,0,period);
    PWMIP_mWriteReg(XPAR_PWMIP_0_S_AXI_BASEADDR,4,period);
    for(int i = 0; i<10000000;i++);
    xil_printf("\n Complete\n");
    cleanup_platform();
    return 0;
}
